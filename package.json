{
  "private": true,
  "name": "kart-track-editor",
  "productName": "Kart Track Editor",
  "description": "Build and edit kart slalom tracks",
  "version": "0.1.0",
  "author": "Kart Track Editor Contributors",
  "main": "app/background.js",
  "repository": "https://github.com/timderes/kart-track-editor",
  "scripts": {
    "dev": "nextron",
    "build": "nextron build",
    "build:all": "nextron build --all",
    "build:win32": "nextron build --win --ia32",
    "build:win64": "nextron build --win --x64",
    "build:mac": "nextron build --mac --x64",
    "build:linux": "nextron build --linux",
    "postinstall": "electron-builder install-app-deps",
    "lint": "eslint . --max-warnings 0 --report-unused-disable-directives",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write .",
    "release": "electron-builder --publish always"
  },
  "dependencies": {
    "@mantine/charts": "^8.1.0",
    "@mantine/core": "^8.1.0",
    "@mantine/dropzone": "^8.1.0",
    "@mantine/form": "^8.1.0",
    "@mantine/hooks": "^8.1.0",
    "@mantine/modals": "^8.1.0",
    "@mantine/notifications": "^8.1.0",
    "@tabler/icons-react": "^3.34.0",
    "electron-log": "^5.4.1",
    "electron-serve": "^1.3.0",
    "electron-store": "^8.2.0",
    "electron-updater": "^6.6.2",
    "embla-carousel": "^8.6.0",
    "embla-carousel-react": "^8.6.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^14.2.30",
    "@types/node": "^20.19.0",
    "@types/react": "^18.3.23",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@workmate/country-and-currency": "^1.2.2",
    "dexie": "^4.0.11",
    "dexie-react-hooks": "^1.1.7",
    "electron": "^33.4.11",
    "electron-builder": "^24.13.3",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "next": "^14.2.30",
    "next-i18next": "^15.4.2",
    "nextron": "^9.5.0",
    "postcss": "^8.5.5",
    "postcss-preset-mantine": "^1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "3.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-image-file-resizer": "^0.4.8",
    "recharts": "^2.12.7",
    "use-elapsed-time": "^3.0.4",
    "uuid": "^9.0.1"
  }
}
